generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

// schema.prisma

model EmailLog {
  id        String   @id @default(cuid())
  to        String
  subject   String
  body      String
  status    String
  error     String?
  createdAt DateTime @default(now())
}
